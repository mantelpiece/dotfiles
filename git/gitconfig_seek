[core]
    excludesfile = /Users/brendan.studds/.gitignore_global
[user]
    name = Brendan Studds
    email = bstudds@seek.com.au
[color]
    ui = true
[color "status"]
    branch = red
    added = green
    changed = blue
    untracked = magenta
[alias]
    lgg = log  -n10 --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s <%ai> %C(bold blue)<%an>%Creset' --abbrev-commit --date=iso
    lggy = log -n10         --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s <%ai> %C(bold blue)<%an>%Creset' --abbrev-commit --date=iso
    staged = diff --cached

    st = status
    cb = checkout -b
    co = checkout
    ap = add -p
    cc = commit
    cm = commit -m
[gpg]
    format = ssh
[gpg "ssh"]
    program = /Applications/1Password.app/Contents/MacOS/op-ssh-sign
    allowedSignersFile = ~/.config/git/allowed_signers
[commit]
    gpgsign = true
[user]
    signingkey = ssh-rsa 
    email = bstudds@seek.com.au
    name = Brendan Studds
